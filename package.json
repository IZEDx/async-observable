  "bugs": {
    "url": "https://github.com/IZEDx/async-observable/issues"
  },
  "homepage": "https://github.com/IZEDx/async-observable#readme",
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "ts-node": "^4.0.1",
    "typedoc": "^0.10.0",
    "typescript": "^2.7"
  },
  "dependencies": {
    "@types/node": "^8.0.53"
  }
}
