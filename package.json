{
  "name": "async-observable",
  "version": "0.0.2",
  "description": "Observable implementation using TypeScript, async/await and AsyncIterables.",
  "main": "dist/es2015/main.js",
  "types": "types/main.d.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "build-es5": "npm run build -- --target es5 --outDir dist/es5",
    "build-es2015": "npm run build -- --target es2015 --outDir dist/es2015",
    "build-es2016": "npm run build -- --target es2016 --outDir dist/es2016",
    "build-es2017": "npm run build -- --target es2017 --outDir dist/es2017",
    "build-esnext": "npm run build -- --target esnext --outDir dist/esnext",
    "build-all": "npm run build-es5 & npm run build-es2015 & npm run build-es2016 & npm run build-es2017 & npm run build-esnext",
    "watch": "npm run build -- --watch",
    "test": "./node_modules/.bin/mocha -p tests -r ts-node/register tests/*.ts",
    "docs": "./node_modules/.bin/typedoc --name async-observable --readme README.md --module commonjs --target ES5 --excludeExternals --out docs --theme default src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IZEDx/async-observable.git"
  },
